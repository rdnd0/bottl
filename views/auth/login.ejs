<div class="container">
<form action="/login" method="POST" id="form">
  
  <h2>Login</h2>
  <a href="/"><img src="/images/wilson.png" class="img" alt="wilson"></a>  
  <div class="login-box"> 
    <input class="login" type="text" name="username" placeholder="user name">
  <div class="pass">
    <input class="login" type="password" name="password" placeholder="password">
  </div>
  <% if (errorMessage) { %>
    <div class="error-message"><%= errorMessage %></div>
  <% } %>
    <button class="btn-login">Let's Go!</button>
  </div>
  <p class="btns-secondary">
    No account yet? <a href="/signup">Sign up</a>
  </p>
</form>
<div class="arrow-left "><a href="/">AAA</a></div>
</div>